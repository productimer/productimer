console.log("trying to join",roomID);










function leaveRoom(previousRoom,jwtID) {
    console.log("previous room inside leaveRoom:",previousRoom)
  if (previousRoom) {
    // Leave the current room
    socket.emit('leave', previousRoom,jwtID);
    console.log(`Left room: ${previousRoom}`);
    
    // Update the previous room variable
    previousRoom = currentRoom;
    // Reset the current room variable
   
  } else {
    console.log('No room to leave.');
  }
}

async function joinRoom(roomID,jwtID){
   
    currentRoom = roomID;
    const jwt = jwtID;

    await socket.emit("joinRoom",roomID,jwtID,(callback)=>{
                    console.log(callback);
                    ownerStatus=callback.isOwner
                    msgBox.style.color="greenyellow"
                    msgBox.innerHTML = `${callback.message}`
                    msgBox.classList.add("glideDown");
                    setTimeout(() => {
                        
                        msgBox.classList.remove("glideDown")
                        msgBox.innerText=""
                    }, 5000);
                    if(roomID!==''){

                        roomDisplay.innerText=`Room:${roomID}`;
                    }
                   


});
console.log(`Joined room: ${currentRoom}`);
await  leaveRoom(previousRoom,jwtID);
previousRoom = currentRoom;

//   console.log('updated previous room to ',previousRoom);

}

